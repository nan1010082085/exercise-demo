{
  "name": "exercise-demo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "type-check": "vue-tsc --noEmit",
    "build:only": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" vite build",
    "build": "pnpm run type-check && pnpm run build:only",
    "build:quiet": "cross-env NODE_OPTIONS=\"--max-old-space-size=4096\" SASS_SILENCE_DEPRECATIONS=legacy-js-api vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "add:commit": "git add . && git cz",
    "commit": "git cz",
    "build:V": "tsx ./build/index.ts",
    "build:full": "pnpm run build:V && pnpm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./cz.config.js"
    }
  },
  "ava": {
    "files": [
      "**/*.{test,spec}.{js,mjs,cjs,ts,mts,cts,jsx,tsx}"
    ],
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@antv/x6": "^2.18.1",
    "@element-plus/icons-vue": "^2.3.1",
    "@jamescoyle/vue-icon": "^0.1.2",
    "@mdi/js": "^7.4.47",
    "@micro-zoe/micro-app": "1.0.0-rc.4",
    "@storybook/test": "8.0.6",
    "@univerjs/presets": "^0.6.10",
    "@vueuse/core": "^10.11.1",
    "@xterm/addon-attach": "^0.11.0",
    "@xterm/addon-fit": "^0.10.0",
    "@xterm/xterm": "^5.5.0",
    "axios": "^1.10.0",
    "axios-mock-adapter": "^1.22.0",
    "dayjs": "^1.11.13",
    "element-plus": "^2.10.2",
    "fast-xml-parser": "^5.2.5",
    "graphql": "^16.11.0",
    "html2canvas": "^1.4.1",
    "jsencrypt": "^3.3.2",
    "leaflet": "^1.9.4",
    "lodash-es": "^4.17.21",
    "motion-v": "^1.4.0",
    "normalcss": "^2.0.5",
    "pdfjs-dist": "^5.3.31",
    "pinia": "^2.3.1",
    "sass": "^1.89.2",
    "tdesign-icons-vue-next": "^0.2.6",
    "tdesign-vue-next": "^1.14.2",
    "uuid": "^9.0.1",
    "vue": "^3.5.17",
    "vue-leaflet": "^0.1.0",
    "vue-pdf-embed": "^2.1.2",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antv/x6-plugin-history": "^2.2.4",
    "@antv/x6-plugin-selection": "^2.2.2",
    "@antv/x6-plugin-snapline": "^2.1.7",
    "@antv/x6-vue-shape": "^2.1.2",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/vue3": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.14",
    "@tsconfig/node18": "^18.2.4",
    "@types/leaflet": "^1.9.19",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.4",
    "@types/uuid": "^9.0.8",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vitejs/plugin-vue-jsx": "^4.2.0",
    "@vitest/ui": "^0.34.7",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cz-git": "^1.11.2",
    "echarts": "^5.6.0",
    "kolorist": "^1.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rollup-plugin-visualizer": "^6.0.3",
    "sass-loader": "^13.0.0",
    "storybook": "^8.6.14",
    "stylelint": "^16.21.0",
    "stylelint-config-standard-scss": "^15.0.1",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^6.3.5",
    "vite-plugin-copy": "^0.1.6",
    "vite-plugin-windicss": "^1.9.4",
    "vitest": "^0.34.6",
    "vue-tsc": "^2.0.29",
    "windicss": "^3.5.6"
  },
  "engines": {
    "node": "18.x"
  }
}
