{
  "name": "exercise-demo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:V -prod && vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "add:commit": "git add . && git cz",
    "commit": "git cz",
    "build:V": "ts-node-esm ./build/index.ts",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./cz.config.js"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@antv/x6": "^2.18.1",
    "@micro-zoe/micro-app": "1.0.0-rc.4",
    "@vueuse/core": "^10.4.1",
    "axios": "^1.5.0",
    "dayjs": "^1.11.10",
    "echarts": "^5.4.3",
    "graphql": "^16.8.1",
    "jsencrypt": "^3.3.2",
    "lodash-es": "^4.17.21",
    "normalcss": "^2.0.5",
    "pinia": "^2.1.6",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.7.0",
    "uuid": "^9.0.1",
    "vue": "^3.3.7",
    "vue-router": "^4.2.4",
    "wujie": "^1.0.22",
    "wujie-vue3": "^1.0.22",
    "xterm": "^5.3.0",
    "xterm-addon-attach": "^0.9.0",
    "xterm-addon-fit": "^0.8.0"
  },
  "devDependencies": {
    "@antv/x6-plugin-history": "^2.2.4",
    "@antv/x6-plugin-selection": "^2.2.2",
    "@antv/x6-plugin-snapline": "^2.1.7",
    "@antv/x6-vue-shape": "^2.1.2",
    "@storybook/addon-essentials": "7.5.2",
    "@storybook/addon-interactions": "7.5.2",
    "@storybook/addon-links": "7.5.2",
    "@storybook/blocks": "7.5.2",
    "@storybook/testing-library": "0.2.2",
    "@storybook/vue3": "7.5.2",
    "@storybook/vue3-vite": "7.5.2",
    "@tsconfig/node18": "^18.2.2",
    "@types/echarts": "^4.9.22",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.6.0",
    "@types/uuid": "^9.0.7",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/ui": "^0.34.4",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.1",
    "kolorist": "^1.8.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sass": "^1.66.1",
    "storybook": "7.5.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
