{
  "name": "exercise-demo",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "pnpm build:V -prod && vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:watch": "vitest watch",
    "add:commit": "git add . && git cz",
    "commit": "git cz",
    "build:V": "ts-node-esm ./build/index.ts"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "czConfig": "./cz.config.js"
    }
  },
  "ava": {
    "extensions": [
      "ts"
    ],
    "require": [
      "ts-node/register"
    ]
  },
  "dependencies": {
    "@vueuse/core": "^10.4.1",
    "axios": "^1.5.0",
    "dayjs": "^1.11.10",
    "graphql": "^16.8.1",
    "lodash-es": "^4.17.21",
    "normalcss": "^2.0.5",
    "pinia": "^2.1.6",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.5.6",
    "vue": "^3.3.7",
    "vue-router": "^4.2.4"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/lodash-es": "^4.17.9",
    "@types/node": "^20.6.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.2",
    "@vitest/ui": "^0.34.4",
    "@vue/tsconfig": "^0.4.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.1",
    "kolorist": "^1.8.0",
    "sass": "^1.66.1",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.2",
    "typescript": "^5.0.2",
    "unplugin-auto-import": "^0.16.6",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.4.5",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.5"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=8"
  }
}
