    "leaflet": "^1.9.4",
    "lodash-es": "^4.17.21",
    "normalcss": "^2.0.5",
    "pdfjs-dist": "^5.1.91",
    "pinia": "^2.1.7",
    "tdesign-icons-vue-next": "^0.2.2",
    "tdesign-vue-next": "^1.9.3",
    "uuid": "^9.0.1",
    "vue": "^3.4.21",
    "vue-leaflet": "^0.1.0",
    "vue-pdf-embed": "^2.1.2",
    "vue-router": "^4.3.0",
    "wujie": "^1.0.22",
